# setup globals
proc init_2_globals {
    @.globals = {};

    @.arg = {};
    @.arg[0] = init_fn_print;
    @.arg[1] = init_fn_type;
    @.arg[2] = pre_userstart;
    @.arg.i = 0;

    jmp init_2_globals_loop;
}

proc init_2_globals_loop {
    %i = @.arg.i;
    @.arg.i = @.arg.i + 1;
    jmp @.arg[%i];
}
