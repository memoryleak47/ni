proc init_singletons {
    @.singletons = {};

    @.arg = {};
    @.arg[0] = init_type_and_object;
    @.arg[1] = init_bool;
    @.arg[2] = init_int;
    @.arg[3] = init_misc;
    @.arg[4] = init_list;
    @.arg[5] = init_globals;
    @.arg.i = 0;

    jmp init_singletons_loop;
}

proc init_singletons_loop {
    %i = @.arg.i;
    @.arg.i = @.arg.i + 1;
    jmp @.arg[%i];
}
